<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ummdeliciousbook"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/load"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.ummdeliciousbook.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity 
            android:name="com.example.ummdeliciousbook.Refrigerator" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.ummdeliciousbook.Search">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            
        </activity>
        <activity
            android:name="com.example.ummdeliciousbook.FoodInput">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            
        </activity>
        <activity 
            android:name="com.example.ummdeliciousbook.Loading"
            android:theme="@android:style/Theme.NoTitleBar"
            ></activity>
        <activity 
            android:name="com.example.ummdeliciousbook.Alarm"
            android:theme="@android:style/Theme.NoTitleBar"
            ></activity>
        <receiver android:name = "AlarmReceiver"></receiver>
        <activity 
            android:name="com.example.ummdeliciousbook.AlarmOffPage"
            ></activity>
    </application>

</manifest>